<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminHomeActivity">

    <Button
        android:id="@+id/btnVerClasesVistaAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver clases disponibles"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <Button
        android:id="@+id/btnVerHorarios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver horarios"
        app:layout_constraintTop_toBottomOf="@+id/btnVerClasesVistaAdmin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <Button
        android:id="@+id/btnVerClasesAsiganadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver clases asignadas"
        app:layout_constraintTop_toBottomOf="@+id/btnVerHorarios"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />




</androidx.constraintlayout.widget.ConstraintLayout>